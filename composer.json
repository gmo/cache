{
	"name": "gmo/cache",
	"description": "Common cache abstraction and utilities",
	"require": {
		"alexey-kupershtokh/predis-phpdoc": "*",
		"gmo/common": "^1.26@dev",
		"predis/predis": "^1.0"
	},
	"require-dev": {
		"gmo/dependency-injection": "~1.0",
		"phpunit/phpunit": "3.7.*"
	},
	"suggest": {
		"gmo/dependency-injection": "Provides a service provider for dependency injection"
	},
	"autoload": {
		"psr-4": {
			"GMO\\Cache\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"GMO\\Cache\\Tests\\": "tests"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.x-dev"
		}
	},
	"repositories": [
		{ "type": "composer", "url": "composer.gmodev.com" }
	]
}
